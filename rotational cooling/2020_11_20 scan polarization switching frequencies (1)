   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "pmt_data = -np.array([pxie[idx][:,pmt_channel].astype(float) for idx in range(1,len(pxie)+1)])\n",
    "pmt_data -= pmt_data[:,-200:].mean(axis = 1)[:, np.newaxis]\n",
    "shutter_data = np.array([pxie[idx][:,shutter_channel].mean() for idx in range(1,len(pxie)+1)])\n",
    "freq_data = np.array([float(pxie_attrs[idx][attr_name]) for idx in range(1,len(pxie)+1)])"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "16*50 shots per frequency random frequency choice (total 4000 shots)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
   "metadata": {},
   "outputs": [],
   "source": [
    "nbunched = 50\n",
    "trace_len = 2500\n",
    "bunches_per_freq = 16"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 46,
   "metadata": {},
   "outputs": [],
   "source": [
    "pmt_data_reshaped = pmt_data.reshape(-1,bunches_per_freq*nbunched,trace_len)\n",
    "frequencies = freq_data.reshape(-1,bunches_per_freq*nbunched).mean(axis = 1)\n",
    "shutter_data_reshaped = shutter_data.reshape(-1,bunches_per_freq*nbunched)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_shutter_slice(val):\n",
    "    state = val > 2e4\n",
    "    if state:\n",
    "        return np.s_[::2], np.s_[1::2]\n",
    "    else:\n",
    "        return np.s_[1::2], np.s_[::2]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 110,
   "metadata": {},
   "outputs": [],
   "source": [
    "integral_limit = -1e7\n",
    "ratios = {f:[] for f in frequencies}\n",
    "for f,d,shut_state in zip(frequencies, pmt_data_reshaped, shutter_data_reshaped):\n",
    "    bunched = d.reshape(-1,nbunched//25,trace_len)\n",
    "    shut_bunched = shut_state.reshape(-1,nbunched//25)\n",
    "    for st, bunch in zip(shut_bunched, bunched):\n",
    "        slice_open, slice_closed = get_shutter_slice(st[0])\n",
    "        mo = bunch[slice_open].mean(axis = 0)\n",
    "        mc = bunch[slice_closed].mean(axis = 0)\n",
    "        io = np.trapz(mo)\n",
    "        ic = np.trapz(mc)\n",
    "        if (io < integral_limit) or (ic < integral_limit):\n",
    "            ratios[f].append(np.nan)\n",
    "        else:\n",
    "            ratios[f].append(io/ic)"
   ]
  },
